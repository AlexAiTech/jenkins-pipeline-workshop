.aui-avatar-badged.avatar-dimmed > .aui-avatar-inner > img {
  transition: opacity 0.3s ease-in-out;
  opacity: 0.5;
}
.aui-avatar-badged .badge {
  transition: all 0.3s ease-in-out;
  -webkit-transform: scale(1);
  transform: scale(1);
  width: 16px;
  height: 16px;
  color: #fff;
  display: block;
  font-size: 9px;
  line-height: 16px;
  opacity: 1;
  position: absolute;
  right: -8px;
  text-align: center;
  text-indent: -999em;
  top: -8px;
}
.aui-avatar-badged.badge-hidden.avatar-dimmed > .aui-avatar-inner > img {
  opacity: 1;
}
.aui-avatar-badged.badge-hidden .badge {
  -webkit-transform: scale(0);
  transform: scale(0);
  opacity: 0;
}
.aui-avatar-badged .approved,
.aui-avatar-badged .needs-work {
  opacity: 1;
  transition: all 0.2s cubic-bezier(0.14, 0.01, 0.65, 1.61);
  transition-property: opacity, transform;
  transform: scale(1);
  width: 16px;
  height: 16px;
  color: #fff;
  background-size: 16px;
  position: absolute;
  right: -8px;
  top: -8px;
  text-align: center;
  display: block;
  font-size: 9px;
  line-height: 16px;
  text-indent: -999em;
}
