/* module-key = 'com.atlassian.bitbucket.server.bitbucket-web:searchable-multi-selector', location = '/static/bitbucket/internal/widget/searchable-multi-selector/searchable-multi-selector.less' */
@keyframes fade {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
.searchable-multi-selector {
  min-height: 32px;
  line-height: 30px;
}
.searchable-multi-selector > ul > li > input.select2-input {
  color: #333;
  box-sizing: content-box;
}
.searchable-multi-selector .select2-choices .select2-search-choice > div {
  max-width: 200px;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.searchable-multi-selector .select2-choices .select2-search-choice > div > div {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.searchable-multi-selector-dropdown {
  margin-top: 0;
}
.searchable-multi-selector-dropdown > ul.select2-results {
  padding: 0;
  margin: 0 0 4px;
  max-height: 185px;
}
.searchable-multi-selector-dropdown > ul.select2-results li.select2-more-results,
.searchable-multi-selector-dropdown > ul.select2-results li.select2-searching,
.searchable-multi-selector-dropdown > ul.select2-results li.select2-no-results {
  margin-top: 5px;
  padding-left: 5px;
  font-size: 13px;
  background-color: #fff;
  color: #999;
}
.searchable-multi-selector-dropdown .select2-result-label {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.searchable-multi-selector-dropdown .select2-result-label > div {
  display: inline;
}
.searchable-multi-selector-dropdown .select2-highlighted {
  background-color: #3b73af;
}
.aui-select2-container.select2-container-multi.select2-container-disabled .select2-choices .select2-search-choice {
  padding: 1px 18px 1px 5px;
}

/* module-key = 'com.atlassian.bitbucket.server.bitbucket-web:group-multi-selector', location = '/static/bitbucket/internal/feature/user/group-multi-selector/group-multi-selector.less' */
@keyframes fade {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
.searchable-multi-selector .group-avatar {
  margin-right: 5px;
}
.searchable-multi-selector-dropdown .group-avatar {
  vertical-align: middle;
  margin-right: 10px;
}

/* module-key = 'com.atlassian.bitbucket.server.bitbucket-web:paged-table', location = '/static/bitbucket/internal/widget/paged-table/paged-table.less' */
@keyframes fade {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
.paged-table-filter {
  background-color: #f5f5f5;
  border: solid #ccc;
  border-width: 1px 0;
}
.paged-table-filter > .aui-item {
  padding: 5px 10px;
}
.paged-table-filter-input {
  box-sizing: border-box;
  background-clip: padding-box;
  border-radius: 3px;
  box-shadow: inset 0 1px 3px #ccc;
  background: white url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAA0AAAANCAYAAABy6+R8AAAAb0lEQVR42p2RMQ7AIAhFWXsG1h7Gtffo6urqyJGpwyetomnwJ38BH/wgfZSaS7PABbVOqkqm3Fzx6IQTatlDb5PJi21YBy1iuNgjJIgzFXqyCcXj1Z1DXNGTiw01aP252DCCgH7Fk41h8KaAGMDxADnaOPucd/m3AAAAAElFTkSuQmCC) no-repeat 7px 7px;
  border: 1px solid #bbb;
  font-family: Arial, FreeSans, Helvetica, sans-serif;
  font-size: 14px;
  height: 30px;
  padding: 5px;
  padding-left: 25px;
  width: 300px;
}
.paged-table-message {
  color: #999;
  padding: 10px;
  text-align: center;
}
.paged-table.no-rows {
  display: none;
}
.paged-table ~ .spinner {
  width: 48px;
  height: 48px;
  margin: 20px auto;
}
.paged-table tr td:first-child {
  border-left: 2px solid transparent;
}
.paged-table tr.focused {
  background-color: #f5f5f5;
}
.paged-table tr.focused td:first-child {
  border-left-color: #3b73af;
}
.paged-table tr.focused a:focus {
  outline: none;
  text-decoration: none;
}

/* module-key = 'com.atlassian.bitbucket.server.bitbucket-web:group-table', location = '/static/bitbucket/internal/feature/user/group-table/group-table.less' */
@keyframes fade {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
.group-table {
  table-layout: fixed;
}
.group-table .group-avatar {
  margin-right: 10px;
}
.group-table #actions {
  width: 36px;
}
.group-table td {
  white-space: nowrap;
}
.group-table td[headers=group] > a {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  max-width: 100%;
  display: inline-block;
  vertical-align: middle;
}
.group-table td[headers=actions] {
  text-align: right;
  vertical-align: middle;
}
.group-table td .group-avatar {
  vertical-align: middle;
}

/* module-key = 'com.atlassian.bitbucket.server.bitbucket-web:user-groups-table', location = '/static/bitbucket/internal/feature/user/user-groups-table/user-groups-table.less' */
@keyframes fade {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
#user-groups-table.no-rows {
  display: table;
}
#user-groups-table .group-avatar {
  margin-right: 10px;
}
#user-groups-table .aui-lozenge-success {
  margin-left: 10px;
}
#user-groups-table .groups-multi-selector-container {
  background-color: #f5f5f5;
  border-top: 1px solid #ccc;
}
#user-groups-table .groups-multi-selector-container .aui-item + .aui-item {
  width: 250px;
}
#user-groups-table .groups-multi-selector-container .groups-multi-selector {
  max-width: none;
  vertical-align: top;
}

/* module-key = 'com.atlassian.bitbucket.server.bitbucket-web:admin-useredit-page', location = '/static/bitbucket/internal/page/admin/users/users.less' */
@keyframes fade {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
#change-password-dialog {
  height: auto !important;
  /* AJS-627: AUI computes the correct height for the internal panels, but not for the dialog's height itself */
}
.user-details-form .error {
  display: block;
  /* ensures error spans are not squashed at the edge of the page - puts them on the line after the inputs */
}
/*create user page*/
form.aui.disabled {
  color: #999;
}
.create-user #content form.aui > .aui-message-error > p {
  display: inline;
}
/* edit user page */
#content .panel-details .aui-item:first-child {
  /* avatar image */
  width: 128px;
}
#content .panel-details .details form {
  position: relative;
  width: 100%;
}
#content .panel-details .buttons a.cancel {
  padding-left: 5px;
}
#content .panel-details .error {
  color: #d04437;
}
#content .panel-details label {
  padding: 0;
}
#content .panel-details #fullname {
  text-overflow: ellipsis;
  overflow: hidden;
