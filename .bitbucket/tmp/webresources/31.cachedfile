/* module-key = 'com.atlassian.bitbucket.server.bitbucket-web:image-explorer', location = '/static/bitbucket/internal/widget/image-explorer/image-explorer.less' */
@keyframes fade {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
.image-explorer-container {
  width: 324px;
}
.image-explorer-container.empty .image-explorer-image-view {
  border: 2px dashed #ccc;
  background: none;
  margin: 0;
}
.image-explorer-container.empty .image-explorer-image-view[data-upload-prompt]:after {
  background-clip: padding-box;
  border-radius: 3px;
  box-sizing: border-box;
  content: attr(data-upload-prompt);
  color: #707070;
  text-align: center;
  font-size: 20px;
  width: 60%;
  height: 100%;
  margin-left: -30%;
  padding: 48px 20px 0;
  position: absolute;
  top: 35%;
  left: 50%;
  background: transparent url(/s/1673131494/eb334ac/1/1.0/_/download/resources/com.atlassian.bitbucket.server.bitbucket-web:image-explorer/icons/icon-downarrow.svg) no-repeat 50% 0%;
  background-size: 48px;
}
.image-explorer-container.empty .image-explorer-image-view .image-explorer-source {
  display: none;
}
.image-explorer-container.empty .image-explorer-image-view .image-explorer-mask {
  display: none;
}
.image-explorer-container.empty .image-explorer-image-view .image-explorer-drag-delegate {
  cursor: default;
}
.image-explorer-container.empty .image-explorer-scale-slider-wrapper {
  display: none;
}
.image-explorer-container.error .image-explorer-image-view[data-upload-prompt]:after {
  display: none;
}
.image-explorer-container .image-explorer-image-view {
  width: 320px;
  height: 320px;
  position: relative;
  overflow: hidden;
  background: url(/s/1673131494/eb334ac/1/1.0/_/download/resources/com.atlassian.bitbucket.server.bitbucket-web:image-explorer/bgCheckerboard.png);
  border: 1px solid #ccc;
  margin: 1px;
}
.image-explorer-container .image-explorer-image-view.active-drop-target {
  border-color: #14892c;
}
.image-explorer-container .image-explorer-image-view.active-drop-target[data-upload-prompt]:after {
  background-image: url(/s/1673131494/eb334ac/1/1.0/_/download/resources/com.atlassian.bitbucket.server.bitbucket-web:image-explorer/icons/icon-tick-inverted.svg);
  background-size: 48px;
  color: #14892c;
}
.image-explorer-container .image-explorer-image-view .image-explorer-drag-delegate {
  width: 100%;
  height: 100%;
  position: absolute;
  cursor: move;
  background-color: rgba(0, 0, 0, 0);
}
.image-explorer-container .image-explorer-image-view .image-explorer-mask {
  box-shadow: 0 0 0 1000px rgba(0, 0, 0, 0.5);
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -128px;
  margin-top: -128px;
}
.image-explorer-container .image-explorer-image-view .image-explorer-mask.circle-mask {
  background-clip: padding-box;
  border-radius: 128px;
  width: 256px;
  height: 256px;
}
.image-explorer-container .image-explorer-image-view .image-explorer-mask.square-mask {
  width: 256px;
  height: 256px;
}
.image-explorer-container .image-explorer-image-view .image-explorer-mask.rounded-square-mask {
  width: 256px;
  height: 256px;
  background-clip: padding-box;
  border-radius: 10px;
}
.image-explorer-container .image-explorer-image-view img {
  position: absolute;
  left: 50%;
  top: 50%;
}
.image-explorer-container .image-explorer-image-view .aui-message {
  position: absolute;
  left: 50%;
  top: 50%;
  box-sizing: border-box;
  margin: 0;
  width: 280px;
  max-height: 280px;
  margin-left: -140px;
  z-index: 10;
  word-wrap: break-word;
}
.image-explorer-container .image-explorer-image-view .aui-message.closeable {
  padding-right: 36px;
}
.image-explorer-container .image-explorer-scale-slider-wrapper {
  display: inline-block;
  width: 270px;
  margin: 20px 25px 0;
  position: relative;
  height: 20px;
  line-height: 20px;
}
.image-explorer-container .image-explorer-scale-slider-wrapper:before,
.image-explorer-container .image-explorer-scale-slider-wrapper:after {
  width: 20px;
  height: 20px;
  content: '';
  background-size: 20px;
  position: absolute;
  top: 50%;
  margin-top: -10px;
  background: transparent no-repeat 50% 50%;
}
.image-explorer-container .image-explorer-scale-slider-wrapper:before {
  left: -25px;
  background-image: url(/s/1673131494/eb334ac/1/1.0/_/download/resources/com.atlassian.bitbucket.server.bitbucket-web:image-explorer/icons/icon-image.svg);
}
.image-explorer-container .image-explorer-scale-slider-wrapper:after {
  right: -27px;
  background-image: url(/s/1673131494/eb334ac/1/1.0/_/download/resources/com.atlassian.bitbucket.server.bitbucket-web:image-explorer/icons/icon-image-large.svg);
}
.image-explorer-container .image-explorer-scale-slider,
.image-explorer-container .fd-slider {
  padding: 0;
  margin: 0;
  display: inline-block;
  width: 270px;
  vertical-align: middle;
}
.image-explorer-container .image-explorer-scale-slider.fd-form-element-hidden,
.image-explorer-container .fd-slider.fd-form-element-hidden {
  display: none;
}
.image-explorer-container .fd-slider .fd-slider-bar {
  left: 3px;
  right: 3px;
}
.image-explorer-container .fd-slider .fd-slider-range {
  left: 4px;
}

/* module-key = 'com.atlassian.bitbucket.server.bitbucket-web:image-upload-and-crop', location = '/static/bitbucket/internal/widget/image-upload-and-crop/image-upload-and-crop.less' */
@keyframes fade {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
.image-upload-and-crop-container.filereader-unsupported .description.fallback {
  display: block;
}
.image-upload-and-crop-container .description.fallback {
  display: none;
}
.image-upload-and-crop-container form.aui {
  width: 324px;
  margin: 20px auto 10px;
  text-align: center;
}
.image-upload-and-crop-container form.aui .spinner {
  width: 30px;
  height: 30px;
  margin-left: 10px;
  position: absolute;
}
.image-upload-and-crop-container .image-explorer-container {
  margin: 0 auto;
}
.image-upload-and-crop-container .image-select-button {
  color: #333;
}
.image-upload-and-crop-container .webcam-capture {
  display: none;
}
.image-upload-and-crop-container.webcam-mode form.aui,
.image-upload-and-crop-container.webcam-mode .image-explorer-container {
  display: none;
}
.image-upload-and-crop-container.webcam-mode .webcam-capture {
  display: block;
}

/* module-key = 'com.atlassian.bitbucket.server.bitbucket-web:user-avatar-form', location = '/static/bitbucket/internal/widget/user-avatar-form/user-avatar-form.less' */
@keyframes fade {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
.user-avatar-form {
  position: relative;
}
.user-avatar-form .avatar-delete-overlay {
  width: 128px;
  height: 128px;
  left: 0;
  opacity: 0;
  position: absolute;
  top: 0;
}
.user-avatar-form .avatar-delete-overlay:hover {
  background-color: #e9e9e9;
  opacity: 0.6;
}
.user-avatar-form .avatar-delete-overlay .avatar-delete-trigger {
  color: #242424;
  display: block;
  margin: 5px;
}
.user-avatar-form .avatar-delete-overlay .avatar-delete-trigger:hover {
  color: #242424;
}
.user-avatar-form .avatar-picker-trigger {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  display: block;
  margin: 10px 0;
}
.user-avatar-form .avatar-picker-trigger,
.user-avatar-form .gravatar-notice {
  width: 128px;
}
.user-avatar-form .gravatar-notice {
  display: none;
  text-align: center;
}
.user-avatar-form .user-avatar {
  margin-right: 0;
}
.gravatar-source .avatar-delete-overlay {
  display: none;
}
.gravatar-source .gravatar-notice {
  display: block;
}

